<% include ../partials/header %> 

<h1>Notifications</h1>

<ul>
	<% allNotifications.forEach(function(notification) { %>
	<li>
	    <%= notification %>
		<a href="/campgrounds/<%= notification.campgroundId %>">
			<strong><%= notification.username %></strong> <em>created a new campground</em>
		</a>
	</li>
	<% }); %>
</ul>

